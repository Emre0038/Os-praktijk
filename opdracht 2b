#include<stdio.h>  
  
void diam(float, float*, float*);  
  
int main()  
{  
    float diameter, circumference;  
  
    printf("Enter radius of Circle in (cm):\n");  
    scanf("%f", &diameter);  
  
    diam(diameter, &circumference);  
  
    printf("omtrek van cirkel = %0.2f\n", circumference);  
  
    return 0;  
}  
  
void diam(float d, float *o)  
{  
    *o = 3.14 *d;  
}

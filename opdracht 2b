#include<stdio.h>  
  
void omtrek(float, float*, float*);  
  
int main()  
{  
    float diameter, cirkelomtrek;  
  
    printf("diameter van cirkel in (cm):\n");  
    scanf("%f", &diameter);  
  
    diam(diameter, &cirkelomtrek);  
  
    printf("omtrek van cirkel = %0.2f\n", cirkelomtrek);  
  
    return 0;  
}  
  
void diam(float d, float *o)  
{  
    *o = 3.14 *d;  
}
